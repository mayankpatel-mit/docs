{
  "openapi": "3.0.1",
  "info": {
    "title": "Survey API",
    "description": "API documentation for managing and retrieving surveys",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://api.yourdomain.com"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "paths": {
    "/surveys": {
      "post": {
        "description": "Get all surveys",
        "requestBody": {
          "description": "Filter surveys",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GetAllSurveys"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "List of surveys"
          }
        }
      }
    },
    "/surveys/one/{survey_id}": {
      "post": {
        "description": "Get single survey",
        "parameters": [
          {
            "name": "survey_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Single survey details"
          }
        }
      }
    },
    "/surveys/{survey_id}/qualification": {
      "post": {
        "description": "Get survey qualification details",
        "parameters": [
          {
            "name": "survey_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Qualification details"
          }
        }
      }
    },
    "/surveys/{survey_id}/link": {
      "post": {
        "description": "Generate survey link",
        "parameters": [
          {
            "name": "survey_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Survey link"
          }
        }
      }
    },
    "/project/get-project-survey-stats/{survey_id}": {
      "get": {
        "description": "Get survey project stats",
        "parameters": [
          {
            "name": "survey_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Project survey stats"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "GetAllSurveys": {
        "type": "object",
        "properties": {
          "authorizationKey": {
            "type": "string"
          },
          "country": {
            "type": "string"
          },
          "language": {
            "type": "string"
          },
          "lengthOfInterviewGTE": {
            "type": "integer"
          },
          "lengthOfInterviewLTE": {
            "type": "integer"
          },
          "collectPII": {
            "type": "boolean"
          },
          "CPIGTE": {
            "type": "integer"
          },
          "CPILTE": {
            "type": "integer"
          },
          "surveyGroupExists": {
            "type": "boolean"
          },
          "incidenceRateGTE": {
            "type": "integer"
          },
          "incidenceRateLTE": {
            "type": "integer"
          }
        }
      }
    }
  }
}
